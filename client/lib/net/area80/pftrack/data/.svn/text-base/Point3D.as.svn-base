package net.area80.pftrack.data
{
	public class Point3D
	{
		public var x:Number;
		public var y:Number;
		public var z:Number;
		
		public function Point3D(x:Number,y:Number,z:Number)
		{
			this.x = x;
			this.y = y;
			this.z = z;
		}
		public function clone ():Point3D {
			return new Point3D(x,y,z);
		}

	}
}