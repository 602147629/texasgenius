package net.area80.ui.skin
{
	import flash.display.MovieClip;
	import flash.display.Sprite;
	import flash.events.Event;

	public class DropdownSkin extends Sprite
	{
		public var selectedLabel:Sprite;
		public var dropButton:MovieClip;
		public var background:MovieClip;
		public var maskMc:Sprite;
		public function DropdownSkin()
		{
			addEventListener(Event.ADDED_TO_STAGE,init,false,0,true);
		}
		private function init(e:Event):void
		{
			background.gotoAndStop(1);			
		}
		
		public function getItem():DropdownItemSkin
		{
			return null;
		}
	}
}