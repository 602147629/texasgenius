package EXIT.engine.player.playervdo
{
	import flash.events.Event;
	
	public class PlayerVdoEvent extends Event
	{
		public static const BUFFER_EMPTY		:String = 'bufferempty';
		public static const BUFFER_FULL			:String = 'bufferfull';
		public static const NOT_FOUND			:String = 'notfound';
		public static const CONNECTED			:String	= 'connected';
		public static const END					:String = 'end';
		public function PlayerVdoEvent(type:String, bubbles:Boolean=false, cancelable:Boolean=false)
		{
			super(type, bubbles, cancelable);
		}
	}
}